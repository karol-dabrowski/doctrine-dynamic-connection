{
  "name": "karol-dabrowski/doctrine-dynamic-connection",
  "description": "Doctrine Dynamic Connection library allows you to modify database connection parameters at runtime",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Karol Dabrowski",
      "email": "contact@karoldabrowski.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "DynamicConnection\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=7.3",
    "doctrine/common": "^3.0",
    "doctrine/dbal": "^2.10.0",
    "doctrine/orm": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "friendsofphp/php-cs-fixer": "^2.18",
    "phpstan/phpstan": "^0.12.81"
  }
}
